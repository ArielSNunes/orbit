{
  "name": "@orbit/jsonapi",
  "version": "0.17.0-beta.7",
  "description": "JSON:API support for Orbit.",
  "contributors": [
    "Dan Gebhardt <dan@cerebris.com>"
  ],
  "keywords": [
    "orbit",
    "orbit.js",
    "JSON:API",
    "JSONAPI",
    "API",
    "REST"
  ],
  "repository": "https://github.com/orbitjs/orbit",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "module": "dist/modules/index.js",
  "types": "dist/modules/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && yarn build:modules && yarn build:commonjs",
    "build:modules": "tsc",
    "build:commonjs": "tsc --project ./tsconfig.commonjs.json",
    "clean": "git clean -x -f",
    "lint": "eslint . --ext .ts",
    "prepare": "yarn build",
    "prepublishOnly": "yarn clean",
    "start": "node ../build dev && snowpack dev",
    "test": "node ../build ci && snowpack build && testem ci"
  },
  "dependencies": {
    "@orbit/core": "^0.17.0-beta.7",
    "@orbit/data": "^0.17.0-beta.7",
    "@orbit/serializers": "^0.17.0-beta.7",
    "@orbit/utils": "^0.17.0-beta.7"
  },
  "devDependencies": {
    "@orbit/build": "^0.17.0-beta.7",
    "@types/sinon": "^9.0.0",
    "sinon": "^9.0.2",
    "whatwg-fetch": "^3.0.0"
  },
  "prettier": "@orbit/prettier-config"
}
